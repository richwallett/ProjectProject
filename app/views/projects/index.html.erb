<h1>All Projects</h1>

<ul id="projects">
  <% @projects.each do |project| %>
    <li>
      <%= link_to "#{project.title}", project_path(project) %>
    </li>
  <% end %>
  <%= link_to "Create a New Project", new_project_path, class: "new_project" %>
  <p><%= link_to "<< Teams", teams_url %></p>
</ul>